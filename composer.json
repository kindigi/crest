{
    "name": "kindigi/crest",
    "description": "A local web development tool for PHP applications.",
    "keywords": ["laravel", "php", "valet", "crest"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bishwajit Adhikary",
            "email": "bishwajitcadhikary@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "cli/includes/compatibility.php",
            "cli/includes/facades.php",
            "cli/includes/helpers.php"
        ],
        "psr-4": {
            "Crest\\": "cli/Crest/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/UsesNullWriter.php",
            "tests/BaseApplicationTestCase.php",
            "tests/Drivers/BaseDriverTestCase.php"
        ]
    },
    "conflict": {
        "mnapoli/silly": ">=1.8.1 <1.8.3"
    },
    "require": {
        "php": "^8.0",
        "illuminate/collections": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/container": "~5.1|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "mnapoli/silly": "^1.5",
        "symfony/console": "^3.0|^4.0|^5.0|^6.0|^7.0",
        "symfony/process": "^3.0|^4.0|^5.0|^6.0|^7.0",
        "guzzlehttp/guzzle": "^6.0|^7.4",
        "symfony/event-dispatcher": "^3.0|^4.0|^5.0|^6.0|^7.0",
        "laravel/prompts": "^0.1.24"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "yoast/phpunit-polyfills": "^0.2.0",
        "laravel/pint": "^1.4"
    },
    "suggest": {
        "ext-pdo": "Required to use the Database driver."
    },
    "bin": [
        "crest"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
